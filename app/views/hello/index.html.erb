<h1>Hello, <%= @user_name %></h1>
<img src="<%= @picture %>">

<h2>Posts</h2>
<% @posts.each do |post| %>
  <div class="postmaster3000">
    <p><%= link_to post.user.full_name, user_path(post.user) %></p>
    <%= image_tag(post.user.picture_url, class: "profile_pic") %>
    <p> <%= post.content %> </p>
  </div>
<% end %>
